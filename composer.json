{
    "name": "orchestra/extension",
    "description": "Extension Component for Orchestra Platform",
    "homepage": "http://orchestraplatform.com/docs/latest/components/extension/",
    "keywords": [
        "orchestra-platform",
        "orchestral",
        "laravel",
        "extension"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Extension\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Extension\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "orchestra/contracts": "7.x-dev",
        "orchestra/memory": "7.x-dev",
        "orchestra/support": "7.x-dev"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/contracts"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/memory"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/support"
        }
    ]
}
