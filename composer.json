{
    "name": "orchestra/extension",
    "description": "Extension Component for Orchestra Platform",
    "homepage": "http://orchestraplatform.com/docs/latest/components/extension/",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "extension"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Extension\\" : "src/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "orchestra/contracts": "~3.4.0",
        "orchestra/memory": "~3.4.0",
        "orchestra/support": "~3.4.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.1",
        "illuminate/console": "~5.4.0",
        "illuminate/container": "~5.4.0",
        "illuminate/filesystem": "~5.4.0",
        "illuminate/http": "~5.4.0"
    },
    "suggest": {
        "laravel/framework": "Allow using orchestra/extension component outside of Orchestra Platform (~5.4).",
        "orchestra/foundation": "Allow using orchestra/extension with Orchestra Platform (~3.4)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.4-dev"
        }
    },
    "minimum-stability": "dev"
}
